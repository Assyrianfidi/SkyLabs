    "@radix-ui/react-tooltip": "^1.2.0",
    "@tanstack/react-query": "^5.60.5",
    "@types/axios": "^0.9.36",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react-google-recaptcha": "^2.1.9",
