{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "build": "node build.js",
    "build:client": "node scripts/build-client.js",
    "build:server": "node scripts/build-server.js",
    "start": "NODE_ENV=production node dist/index.js",
    "start:prod": "cross-env NODE_ENV=production pm2 start dist/index.js --name skylabs --time --update-env",
    "stop:prod": "pm2 delete skylabs",
    "restart:prod": "npm run build && npm run stop:prod && npm run start:prod",
    "logs:prod": "pm2 logs skylabs --lines 100",
    "status:prod": "pm2 status",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:update": "vitest run --update",
    "test:debug": "vitest --inspect-brk",
    "test:e2e": "dotenv -e .env.test npx playwright test",
    "test:e2e:ui": "dotenv -e .env.test npx playwright test --ui",
    "test:e2e:debug": "dotenv -e .env.test npx playwright test --debug",
    "test:e2e:update-snapshots": "dotenv -e .env.test npx playwright test --update-snapshots",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:db": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest server/__tests__/db",
    "test:db:watch": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --watch server/__tests__/db",
    "test:ci": "npm run test:coverage && npm run test:integration && npm run test:db && npm run test:e2e",
    "check": "tsc",
    "config:generate": "tsx scripts/generate-secrets.ts",
    "db:monitor": "tsx server/services/databaseMonitor.ts",
    "db:health": "tsx server/services/dbHealthCheck.ts",
    "db:migrate:apply": "tsx scripts/apply-migrations.ts",
    "db:migrate:create": "drizzle-kit generate:pg --schema=./shared/schema.ts",
    "db:migrate:run": "drizzle-kit migrate:pg --schema=./shared/schema.ts",
    "db:push": "drizzle-kit push:pg --schema=./shared/schema.ts",
    "db:studio": "drizzle-kit studio --port 4000",
    "db:setup": "tsx scripts/setup-db.ts",
    "db:verify": "tsx scripts/verify-migrations.ts",
    "db:init": "npm run config:generate && npm run db:setup && npm run db:verify",
    "db:reset": "npm run db:setup && npm run db:push",
    "db:types": "tsx scripts/generate-types.ts",
    "db:seed": "tsx scripts/seed-db.ts",
    "db:check": "tsx scripts/check-db.ts",
    "db:health-check": "tsx scripts/check-connection.ts",
    "db:validate": "tsx scripts/validate-db-connection.ts",
    "test:db:setup": "node --experimental-specifier-resolution=node scripts/simple-db-setup.js && tsx scripts/create-contacts-table.ts",
    "test:db:create": "node --experimental-specifier-resolution=node scripts/simple-db-setup.js",
    "test:prod": "node --experimental-specifier-resolution=node scripts/test-production.js"
  },
